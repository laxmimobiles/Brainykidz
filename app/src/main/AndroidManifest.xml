<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mycom.example.harshu.janavi"
    android:versionCode="2"
    android:versionName="1.11">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/GreenTheme">
        <activity android:name="mycom.example.harshu.janavi.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mycom.example.harshu.janavi.NumberActivity"
            android:label="@string/numbers"
            android:parentActivityName="mycom.example.harshu.janavi.WelcomeActivity" />
        <activity
            android:name="mycom.example.harshu.janavi.MainActivity"
            android:label="@string/phonics_button_string"
            android:parentActivityName="mycom.example.harshu.janavi.WelcomeActivity" />
        <activity
            android:name="mycom.example.harshu.janavi.SightWordActivity"
            android:label="@string/sight_words_button_string"
            android:parentActivityName="mycom.example.harshu.janavi.WelcomeActivity" />
        <activity
            android:name="mycom.example.harshu.janavi.AdvancedWordsActivity"
            android:label="@string/oxford"
            android:parentActivityName="mycom.example.harshu.janavi.WordListActivity" />
        <activity
            android:name="mycom.example.harshu.janavi.WordListActivity"
            android:label="@string/advancedwords_button_string"
            android:parentActivityName="mycom.example.harshu.janavi.AdvancedWordsActivity" />
    </application>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>